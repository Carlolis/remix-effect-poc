{
  "cSpell.words": [
    "clsx",
    "Freshdesk",
    "hookform",
    "isbot",
    "oidc",
    "passwordless",
    "signup",
    "Signup",
    "tailwindcss",
    "typecheck",
    "zammad",
    "Zammad",
    "zitadel",
    "Zitadel"
  ],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    "source.fixAll.eslint": "always"
  },
  "editor.formatOnSave": true,
  "typescript.tsdk": "node_modules/typescript/lib"
}
